<!doctype html>
<!--[if IE 7]>
<html class="ie ie7" lang="en">
<![endif]-->
<!--[if IE 8]>
<html class="ie ie8" lang="en">
<![endif]-->
<!--[if !(IE 7) | !(IE 8) ]><!-->
<html lang="en">
	<head>
		<meta charset="{{site.charset}}">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="description" content="{{site.description}}">
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0">
		<title>   
			{%- if wp_title -%}
				{{- wp_title -}} - {{- site.name -}}
			{%- else -%}
				{{- site.name -}}
			{%- endif -%}
		</title>

		<!-- Add to homescreen for Chrome on Android -->
		<meta name="mobile-web-app-capable" content="yes">
		<link rel="icon" sizes="192x192" href="images/touch/chrome-touch-icon-192x192.png">

		<!-- Add to homescreen for Safari on iOS -->
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-status-bar-style" content="black">
		<meta name="apple-mobile-web-app-title" content="Material Design Lite">
		<link rel="apple-touch-icon-precomposed" href="apple-touch-icon-precomposed.png">

		<!-- Tile icon for Win8 (144x144 + tile color) -->
		<meta name="msapplication-TileImage" content="img/touch/ms-touch-icon-144x144-precomposed.png">
		<meta name="msapplication-TileColor" content="#3372DF">

		<link rel="profile" href="http://gmpg.org/xfn/11">
		<link rel="stylesheet" href="{{site.theme.link}}/css/bootstrap.min.css" type="text/css">
		<link rel="stylesheet" href="{{site.theme.link}}/css/bootstrap-theme.min.css" type="text/css">
		<link rel="stylesheet" href="{{site.theme.link}}/css/ionicons.min.css" type="text/css">
		<link rel="stylesheet" href="{{site.theme.link}}/css/magnific-popup.min.css" type="text/css">
		<link rel="stylesheet" href="{{site.theme.link}}/css/animate.css" type="text/css">

		{{function('wp_head')}}
	</head>

	<body> 
		<div id="nav-toggle"><span></span></div>

		<div class="main-menu">
			<div class="user-name">James and Jeff</div>
			<nav>{{ function('wp_nav_menu') }}</nav>
		</div>

		<div id="preloader">
		<div id="canvasloader-container"></div>
	</div>




